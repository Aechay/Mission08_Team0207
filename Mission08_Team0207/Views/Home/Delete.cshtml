@model Mission08_Team0207.Models.TaskItem
<h1>Confirm Deletion</h1>
<h3>Are you sure you want to delete this task?</h3>
<form method="post">
    <input type="hidden" asp-for="TaskId"></input>
    <button type="submit">Delete</button>
    <a class="btn btn-info" asp-controller="Home" asp-action="Index">Cancel</a>
</form>
